			window.open(url,'Chat','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=650,height=500,top=20,left=20');
		}

		// Email Sign Up
		function emailSignUp() {
			var params = new Object ();
     			params['type']=4;
			if (navigator.appName == 'Microsoft Internet Explorer') {
				params['width']=348;
				params['height']=220;
			} else {
				params['width']=370;
				params['height']=242;
			}
			var sendurl='email_list_mm.cfm';
			showIbox(sendurl,'',params);
			showBG();
			window.onscroll = maintPos;
			window.onresize = maintPos;
		}

		function trim(str) {
			var str = str.replace(/^\s\s*/, ''),
			ws = /\s/,
			i = str.length;
			while (ws.test(str.charAt(--i)));
			return str.slice(0, i + 1);
		}

		function toggleUpload() {
       			var log = document.getElementById('loginId');

			if (log.style.display=="") log.style.display = "none";
			else log.style.display = "";
		}
	</script>

