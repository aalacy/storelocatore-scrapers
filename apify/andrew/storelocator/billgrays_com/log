<script language="javascript">
	function isPage(pg) {
             if (jspage.toLowerCase()==pg.toLowerCase()) {
                        return true;
             }

             return false;
	}

function isPagePath(pg) {
             if (jspagepath.toLowerCase()==pg.toLowerCase()) {
                        return true;
             }

             return false;
	}

       function LoadDefaults() {
          if (isPage('Home')) menutab = "Tab1";
            else if (isPage('Bill Grays Locations') || isPage('Boulevard Mall')  || isPage('Brockport') || isPage('Chili') || isPage('Clarence') || isPage('Gates') || isPage('Greece') || isPage('Henrietta') || isPage('Irondequoit') || isPage('MCC Sports Center') || isPage('Ontario') || isPage('Ontario Tap Room') || isPage('Penfield') || isPage('Perinton') || isPage('Sea Breeze') || isPage('Strong Museum') || isPage('Webster')) menutab = "Tab2";
            else if (isPage('Bill Grays Menu')) menutab = "Tab3";
            else if (isPage('Coupons')) menutab = "Tab4";
            else if (isPage('Online Store') || isPage('Products') || isPage('Gift Card')) menutab = "Tab5";


if (menutab.length) {
link = "Link" + menutab;
               
document.getElementById(menutab).style.backgroundColor='#CE000C'; document.getElementById(link).style.color="#ffffff";
 }
      }

      LoadDefaults();
</script>
