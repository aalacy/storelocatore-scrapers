<script type="text/javascript">

function fixTW(f,s) {
		if (arguments.length!=2) {
			var f = 0, s = 0;

			if (document.getElementsByClassName('twitter-follow-button')) f = 1;
			if (document.getElementsByClassName('twitter-share-button')) s = 1;

			if (f==1 || s==1) {
				if (typeof fetch=='function') {
					fetch = null;
					loadTW();
					fixTW(f,s);
				} else {
					loadTW();
				}
			}
		} else {
			if (!document.getElementById('twitter-widget-0') || (f==1 && s==1 && !document.getElementById('twitter-widget-1'))) {
				setTimeout(function(){ fixTW(f,s); }, 100);
			} else {
				$('#twitter-widget-0').ready(function(){
					if (f==1 && s==1) {
						$('#twitter-widget-1').ready(function(){
							fixFetch();
						});
					} else {
						fixFetch();
					}
				});
			}
		}
	}

	function fixFetch() {
		fetch = function(url){window.open(url,'_blank','toolbar=no,location=no,directories=no,status=no,menubar=no,scrollbars=yes,resizable=yes,width=1000,height=800,top=0,left=20')};
	}

fixTW();

function JSFX_FloatTopDiv()
{
	var startX = 50,
	startY = 50;
	var ns = (navigator.appName.indexOf("Netscape") != -1);
	var d = document;
	function ml(id)
	{
		var el=d.getElementById?d.getElementById(id):d.all?d.all[id]:d.layers[id];
		if(d.layers)el.style=el;
		el.sP=function(x,y){this.style.left=x;this.style.top=y;};
		el.x = startX;
		el.y = startY;
		return el;
	}
	window.stayTopLeft=function()
	{
		var pY = ns ? pageYOffset : document.body.scrollTop;
		ftlObj.y += (pY + startY - ftlObj.y)/8;
		ftlObj.sP(ftlObj.x, ftlObj.y);
		setTimeout("stayTopLeft()", 10);
	}
	ftlObj = ml("popupDiv");
	stayTopLeft();
}
JSFX_FloatTopDiv();
</script>


