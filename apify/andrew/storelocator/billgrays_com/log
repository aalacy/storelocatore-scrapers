			<script language="javascript">
  var menutimeout = 500;
  var menuclosetimer = 0;
  var menudiv = 0;
  var menutd = 0;
  var menutab = "";
  var activetab = 0;
  var previoustab = 0;

  function menuOpen(n) {
    menutd = "TabDiv"+n;
    activetab = "Tab"+n;

    // cancel close timer
    menuCancelCloseTime();

    // close previous div if new
    if (menudiv && menudiv!=document.getElementById(menutd)) {
      highLightOff(previoustab);
      menudiv.style.display = 'none';
      document.getElementById('containerRow').style.display = 'none';
    }

    previoustab = activetab;
    menudiv = document.getElementById(menutd);

    // open new div if hidden
    if (menudiv.style.display=='none') {
      highLightOn(activetab);
      toggleSlide(menutd);
      document.getElementById('containerRow').style.display = '';
    }
  }

  function menuClose() {
    // close open div
    if (menudiv) {
      highLightOff(previoustab);
      menudiv.style.display = 'none';
    }
  }

  function menuCloseTime() {
    // set timer to close menu div
    menuclosetimer = window.setTimeout(menuClose, menutimeout);
  }

  function menuCancelCloseTime() {
    if (menuclosetimer) {
      // cancel close timer
      window.clearTimeout(menuclosetimer);
      menuclosetimer = 0;
    }
  }

  function highLightOn(tab) {
link = "Link" + tab;
document.getElementById(tab).style.backgroundColor="#CE000C"; document.getElementById(link).style.color="#ffffff";
  }

  function highLightOff(tab) {
    link = "Link" + tab;
    if (tab!=menutab) { document.getElementById(tab).style.backgroundColor="#F7CB8D"; document.getElementById(link).style.color="#7E4A01";
    }
  }

  </script>
