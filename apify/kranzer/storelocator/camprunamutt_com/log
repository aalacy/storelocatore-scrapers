<script>

var $checklogin = "";
if($checklogin != "") {$("#login").html("log out").removeClass("loginButton").addClass("logoutButton").trigger("create")}


				//$("#login").click(function(){ $("#login_panel").animate({top: '25px'}); });
				
				var H=H==0?-100:0;
    $("body.live #login.loginButton").on('click', function(){
        $("#login_panel").animate({top: H}, 300, function() {H=H==-0?-150:0;});
    });
		
	$("body.live #login.logoutButton").on('click', function(){
		location.href = "logout.php";
    });	
	
	
// HOME NAVIGATION
				if($("body").hasClass("live")){
  $("#cramLogo").each(function(){
    var $newLink= $(this).attr("data-live");
	//alert($newLink);
    $(this).attr("href", $newLink)
  });
}
else if($("body").hasClass("edit")){
   $("#cramLogo").each(function(){
    var $newLink= $(this).attr("data-edit");
	//alert($newLink);
    $(this).attr("href", $newLink)
  });
}	

// CHANGE MASTHEAD PHOTO TRIGGER
$('body').on('click', '.openWindow', function() {
$("#photoEditorWindow").animate({top: '-2px'});
  //$("#photoEditor").html("<div class='closeWindow'></div>").trigger("create");
  $(this).addClass('closeWindow').removeClass('openWindow');
});

$('body').on('click', '.closeWindow', function() {
$("#photoEditorWindow").animate({top: '-100px'});
  //$("#photoEditor").html("<div class='openWindow'>open</div>").trigger("create");
  $(this).addClass('openWindow').removeClass('closeWindow');
});
			
</script>                
                
