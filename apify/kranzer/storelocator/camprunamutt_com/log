// HOLIDAY HOURS EXPIRES
	//var date1 = new Date($("#holidayHours").attr("data-expires"));
	//var date2 = new Date($("#holidayHours").attr("data-currentdate"));
	//var diffDays = parseInt((date1 - date2) / (1000 * 60 * 60 * 24)); 
	//if(diffDays < 1) {$("#holidayHours").css("display","none")}
function dateCheck() {

    var fDate,lDate,cDate;
    fDate = new Date($("#holidayHours").attr("data-starts"));   
    lDate = new Date($("#holidayHours").attr("data-expires"));
    cDate =  new Date($("#holidayHours").attr("data-currentdate"));
    if((cDate <= lDate && cDate >= fDate)) {
        $("#holidayHours").css("display","block");
        return true;
    }
    $("#holidayHours").css("display","none");
    return false;
} dateCheck();	
	

// HOME PROMO EXPIRES
var date1 = new Date($("#homePromoLede").attr("data-expires"));
var date2 = new Date($("#holidayHours").attr("data-currentdate"));
var diffDays = parseInt((date1 - date2) / (1000 * 60 * 60 * 24)); 
if(diffDays < 1) {$("#homePromoLede").css("display","none")}

